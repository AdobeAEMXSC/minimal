{
  "supportscredentials":false,
  "supportedmethods":[
    "GET",
    "HEAD",
    "POST"
  ],
  "alloworiginregexp":[
    "https://.*.csb.app",
    "https://cdpn.io/.*",
    "https://www.your-domain.com/.*"
  ],
  "supportedheaders":[
    "Origin",
    "Accept",
    "X-Requested-With",
    "Content-Type",
    "Access-Control-Request-Method",
    "Access-Control-Request-Headers"
  ],
  "allowedpaths":[
    "/graphql/execute.json/.*",
    "/content/.*",
    "/conf/.*"
  ]
}